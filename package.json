{
    "name": "matuc-lti-exercise-composer-backend",
    "version": "1.0.0",
    "description": "Mathematical Exercise Composer - Backend Server for LTI Canvas Integration",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "dev:debug": "nodemon --inspect src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "start:prod": "NODE_ENV=production node dist/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "migrate": "ts-node scripts/migrate-db.ts",
        "seed": "ts-node scripts/seed-db.ts",
        "setup:dev": "./scripts/setup-dev.sh",
        "build:prod": "./scripts/build-production.sh",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "express": "^4.18.2",
        "mongoose": "^7.6.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "express-validator": "^7.0.1",
        "socket.io": "^4.7.4",
        "mathjs": "^12.1.0",
        "express-fileupload": "^1.4.0",
        "morgan": "^1.10.0",
        "compression": "^1.7.4",
        "winston": "^3.11.0",
        "uuid": "^9.0.1",
        "ltijs": "^5.9.0",
        "axios": "^1.6.0",
        "node-jose": "^2.2.0",
        "rate-limiter-flexible": "^3.0.0",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5",
        "ioredis": "^5.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/mongoose": "^5.11.97",
        "@types/cors": "^2.8.17",
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/compression": "^1.7.5",
        "@types/node": "^20.8.10",
        "@types/uuid": "^9.0.7",
        "@types/express-fileupload": "^1.4.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "@types/jest": "^29.5.8",
        "@types/supertest": "^2.0.16",
        "ts-jest": "^29.1.1",
        "nodemon": "^3.0.1",
        "typescript": "^5.2.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "^8.53.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "lti",
        "canvas",
        "mathematics",
        "education",
        "exercise",
        "assessment",
        "nodejs",
        "typescript",
        "mongoose",
        "express"
    ],
    "author": "Wolfgang Rivera <wcrivera@uc.cl>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/wcrivera/matuc-lti-exercise-composer.git"
    }
}